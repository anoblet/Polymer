{
  "entrypoint": "index.html",
  "shell": "src/app-root.html",
  "fragments": [
    "src/styles/global-styles.html",
    "src/styles/flex-styles.html",
    "src/app/styles/app-styles.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "bower.json",
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
