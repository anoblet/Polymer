{
  "entrypoint": "index.html",
  "shell": "src/app-root.html",
  "fragments": [
    "src/styles/global-styles.html",
    "src/styles/flex-styles.html",
    "src/app/styles/app-styles.html",
    "src/app/pages/app-home.html",
    "src/app/blocks/my-city.html",
    "src/app/blocks/my-county.html",
    "src/app/blocks/my-state.html",
    "src/blocks/x-map.html",
    "src/app/pages/page-state.html",
    "src/components/app-data.html",
    "src/components/address-lookup/address-lookup.html"
  ],
  "sources": [
    "images/**/*"
  ],
  "extraDependencies": [
    "bower.json",
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
